{
  "manifest_version": 2,
  "version": "1.0",
  "name": "Env Switcher",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "cookies",
    "tabs",
    "<all_urls>"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "addon@example.com",
      "strict_min_version": "42.0"
    }
  }
}